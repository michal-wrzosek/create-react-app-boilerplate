{
  "name": "create-react-app-boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "25.2.1",
    "@types/node": "13.13.2",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "typescript": "3.8.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test:unit": "react-scripts test",
